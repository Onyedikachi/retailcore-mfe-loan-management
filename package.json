{
  "name": "@Sterling/loan-management",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "start:standalone": "cross-env NODE_ENV=development webpack serve --env standalone",
    "standalone:https": "yarn start:standalone --https && https-server -S -C cert.pem -K key.pem",
    "build": "cross-env NODE_ENV=production concurrently yarn:build:*",
    "build:webpack": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint \"src/**/*.+{js|ts|jsx|tsx}\" --quiet --fix",
    "lint:cache": "eslint --cache --fix",
    "format": "npx prettier --write \"src/**/*.{jsx,tsx,ts,js}\"",
    "check-format": "npx prettier --check \"src/**/*.{jsx,tsx,ts,js}\"",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "build:types": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.0.1",
    "@types/object-mapper": "^6.2.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/systemjs": "^6.1.1",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.53.0",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "ts-config-single-spa": "^3.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-config-single-spa-react": "^4.0.0",
    "webpack-config-single-spa-react-ts": "^4.0.0",
    "webpack-config-single-spa-ts": "^4.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@mui/system": "^5.13.7",
    "@types/react-beautiful-dnd": "^13.1.4",
    "chart.js": "^4.3.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "formik": "^2.4.2",
    "object-mapper": "^6.2.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^17.0.2",
    "react-drag-drop-files": "^2.3.10",
    "react-http-query": "^1.1.1",
    "react-router-dom": "^6.14.1",
    "single-spa": "^5.9.3",
    "single-spa-react": "^4.3.1",
    "yup": "^1.2.0"
  },
  "lint-staged": {
    "src/**/*.{ts,jsx,jsx,js}": [
      "prettier --ignore-path ./.prettierignore --write",
      "eslint"
    ]
  },
  "types": "dist/Sterling-boilerplate.d.ts",
  "resolutions": {
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9"
  }
}
